                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ address: comp.address })
                    });

                    if (response.ok) {
                        const details = await response.json();
                        setModalDetails(details);
                    } else {
                        setModalDetails({ error: 'Failed to fetch property details' });
                    }
