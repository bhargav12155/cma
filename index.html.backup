            const getConditionColor = (condition) => {
                const conditionMap = {
                    'Excellent': 'text-green-600',
                    'Good': 'text-blue-600',
                    'Average': 'text-yellow-600',
                    'Fair': 'text-orange-600',
                    'Poor': 'text-red-600'
